#include <stdio.h>

int main(){
int j = 1;
int i = 1;
int k = 1;
    for( i = 1; i <= 5; i++){

       printf("direita\n");

    }

printf("/////////////////////\n");

while(j <= 5){

printf("cima\n");
printf("direita\n");
j++;


}

printf("/////////////////////\n");


do{

printf("esquerda\n");
k++; 

}while(k <= 8);


printf("/////////////////////\n");

















}
